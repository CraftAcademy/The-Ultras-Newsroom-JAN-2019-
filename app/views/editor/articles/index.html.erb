<% @articles.each do |article|%>
    <%= link_to(editor_article_path(article)) do %>
        <div class="max-w-sm rounded">
          <div class="px-6 py-4">
            <div class="font-bold text-xl mb-2">
              <%= article.title %>
            </div>
            <p class="text-grey-darker text-base">
              <%= article.lead %>
            </p>
          </div>
        </div>
    <% end %>
<% end %>

<%= link_to ('Home'), root_path %>